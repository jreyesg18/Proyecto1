<h1>Parámetros</h1>
<%= link_to 'Nuevo Parámetro', new_parametro_path %>
<table>
  <thead>
  <tr>
    <th>Género</th>
    <th>Grado de Tumor</th>
    <th>Pleura</th>
    <th>Tipo Histológico</th>
    <th>Edad</th>
    <th>Abdominal</th>
    <th>Mama</th>
    <th>Mediastino</th>
    <th>Cerebro</th>
    <th>Hueso</th>
    <th>Peritoneum</th>
    <th>Piel</th>
    <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
  <% @parametros.each do |parametro| %>
    <tr>
      <td><%= parametro.genero %></td>
      <td><%= parametro.grado_de_tumor %></td>
      <td><%= parametro.pleura %></td>
      <td><%= parametro.tipo_histologico %></td>
      <td><%= parametro.edad %></td>
      <td><%= parametro.abdominal %></td>
      <td><%= parametro.mama %></td>
      <td><%= parametro.madiastino %></td>
      <td><%= parametro.cerebro %></td>
      <td><%= parametro.hueso %></td>
      <td><%= parametro.peritoneum %></td>
      <td><%= parametro.piel %></td>
      <td><%= link_to 'Mostrar', parametro %></td>
      <td><%= link_to 'Editar', edit_parametro_path(parametro) %></td>
      <td><%= link_to 'Eliminar', parametro, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>
